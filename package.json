{
  "name": "react-redux-starter-kit",
  "version": "1.0.0",
  "description": "Basic setup for web app with React and Redux",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel server lint:watch",
    "server": "babel-node buildScripts/devServer.js",
    "webpack": "babel-node webpack --config webpack.config.dev.js",
    "test": "jest --no-cache",
    "lint": "esw webpack.config.* src buildScripts",
    "lint:watch": "npm run lint --watch",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/prodServer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sliultron/React-Redux-Starter-Kit.git"
  },
  "author": "Sean Li",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sliultron/React-Redux-Starter-Kit/issues"
  },
  "homepage": "https://github.com/sliultron/React-Redux-Starter-Kit#readme",
  "jest": {
  
    "unmockedModulePathPatterns": [
      "react",
      "prop-types",
      "lodash",
      "react-dom"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "collectCoverage": true,
    "unmockedModulePathPatterns":[
      "react",
      "lodash",
      "prop-types",
      "react-dom"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-watch": "^3.0.1",
    "express": "^4.15.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "npm-run-all": "^4.0.2",
    "open": "0.0.5",
    "regenerator-runtime": "^0.10.3",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^3.3.7",
    "connect-history-api-fallback": "^1.3.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.6",
    "react": "^15.5.3",
    "react-dom": "^15.5.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.4",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  }
}
